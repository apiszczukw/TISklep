@model IEnumerable<TISklep.Models.Film>
@{
    
}

<table>
    <thead>
        <tr>
            <th>Tytuł filmu</th>
            <th>Reżyser</th>
            <th>Data produkcji</th>
            <th>Cena</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var film in Model)
        {
            <tr>
                <td>@film.Tytul</td>
                <td>@film.Rezyser</td>
                <td>@film.DataProdukcji</td>
                <td>@film.Cena</td>
            </tr>
        }
    </tbody>
</table>